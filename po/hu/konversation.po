msgid "There is a problem in the output format configuration."
msgstr "Hiba van a kimeneti formátum beállításában."

#: data/scripts/media:315
msgid "media v%1 for Konversation."
msgstr "média v%1 a Konversationhöz."

#: data/scripts/media:316
msgid "Usage:"
msgstr "Használat:"

#: data/scripts/media:317
msgid "\"/media\" - report what the first player found is playing."
msgstr ""
"\"/media\" - jelentés arról, hogy az első talált lejátszó éppen mit játszik."

#: data/scripts/media:318
msgid ""
"\"/media [ 'audio' | 'video' ]\" - report what is playing in a known audio "
"or video player."
msgstr ""
"\"/media [ 'audio' | 'video' ]\" - jelentés arról, hogy éppen mit játszik az "
"ismert hang- vagy videólejátszó."

#: data/scripts/media:319
msgid ""
"\"/media { Player }\" - report what is playing in the specified player if it "
"is found."
msgstr ""
"\"/media { Player }\" - jelentés arról, hogy éppen mit játszik a megadott "
"lejátszó, ha az megtalálható."

